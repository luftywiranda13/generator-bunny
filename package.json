{
  "name": "generator-bunny",
  "version": "0.0.0-development",
  "description": "Jumpstart node module, like a bunny",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luftywiranda13/generator-bunny.git"
  },
  "preferGlobal": true,
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.instagram.com/luftywiranda13/"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "lint-staged",
    "test": "nps test"
  },
  "main": "app/index.js",
  "files": [
    "app/index.js",
    "app/templates/"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "bunny",
      "bunny/recommended"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "2.1.0",
    "lodash.camelcase": "4.3.0",
    "lodash.kebabcase": "4.1.1",
    "shelljs": "0.7.8",
    "yeoman-generator": "1.1.1"
  },
  "devDependencies": {
    "all-contributors-cli": "4.4.0",
    "babel-cli": "6.24.1",
    "babel-preset-env": "1.6.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.4.0",
    "eslint-config-bunny": "4.0.1",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "lint-staged": "4.0.3",
    "nps": "5.7.0",
    "nps-utils": "1.3.0",
    "opt-cli": "1.5.1",
    "validate-commit-msg": "2.14.0",
    "yeoman-assert": "3.0.0",
    "yeoman-test": "1.7.0"
  },
  "keywords": [
    "üê∞",
    "javascript",
    "open-source",
    "node",
    "node-module",
    "yeoman",
    "yeoman-generator",
    "starter-kit",
    "boilerplate",
    "scaffold",
    "npm",
    "automated",
    "automate",
    "easy",
    "jest",
    "babel",
    "eslint"
  ]
}
