{
  "name": "solid-node-module",
  "version": "0.0.0-development",
  "description": "Instant node module starter-kit",
  "license": "MIT",
  "author": "Lufty Wiranda <lufty.wiranda@gmail.com>",
  "homepage": "https://github.com/luftywiranda13/solid-node-module#readme",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "nps",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "lint-staged && opt --in pre-commit --exec \"npm start validate\"",
    "test": "nps test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "autoFix": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint -- --trailing-comma all --single-quote --parser flow --write",
      "git add"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "npm",
    "module"
  ],
  "bugs": {
    "url": "https://github.com/luftywiranda13/solid-node-module/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luftywiranda13/solid-node-module.git"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^20.0.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.0",
    "flow-bin": "^0.45.0",
    "husky": "^0.13.2",
    "jest-cli": "^20.0.0",
    "lint-staged": "^3.3.1",
    "nps": "^5.0.3",
    "nps-utils": "^1.1.2",
    "opt-cli": "^1.5.1",
    "prettier-eslint-cli": "^3.1.2",
    "react-test-renderer": "^15.5.4",
    "semantic-release": "^6.3.2",
    "validate-commit-msg": "^2.11.1"
  }
}
