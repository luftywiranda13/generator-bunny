{
  "name": "generator-bunny",
  "version": "0.0.0-development",
  "description": "Jumpstart node module, like a bunny",
  "license": "MIT",
  "author": "Lufty Wiranda <lufty.wiranda@gmail.com>",
  "homepage": "https://github.com/luftywiranda13/generator-bunny#readme",
  "bugs": "https://github.com/luftywiranda13/generator-bunny/issues",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luftywiranda13/generator-bunny.git"
  },
  "main": "app/index.js",
  "files": [
    "app/index.js",
    "app/templates/"
  ],
  "scripts": {
    "start": "nps",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "lint-staged",
    "test": "nps test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "keywords": [
    "üê∞",
    "javascript",
    "open-source",
    "node",
    "node-module",
    "yeoman",
    "yeoman-generator",
    "starter-kit",
    "boilerplate",
    "scaffold",
    "npm",
    "automated",
    "automate",
    "easy",
    "jest",
    "babel",
    "prettier",
    "eslint"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "hasbin": "^1.2.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.3.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.23.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "nps": "^5.3.1",
    "nps-utils": "^1.1.2",
    "opt-cli": "^1.5.1",
    "prettier": "^1.4.4",
    "validate-commit-msg": "2.12.2",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  }
}
